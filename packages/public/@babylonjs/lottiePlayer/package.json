{
    "name": "@babylonjs/lottie-player",
    "version": "8.30.3",
    "main": "index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "esnext": "index.js",
    "description": "Babylon.js Lottie Player",
    "keywords": [
        "animation",
        "2D",
        "javascript",
        "html5",
        "webgl",
        "webgl2",
        "webgpu",
        "babylon",
        "lottie"
    ],
    "license": "MIT",
    "readme": "README.md",
    "type": "module",
    "sideEffects": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BabylonJS/Babylon.js.git"
    },
    "files": [
        "**/*.js",
        "**/*.d.ts",
        "**/*.map",
        "./*.tgz",
        "readme.md",
        "license.md",
        "!build/**"
    ],
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "rimraf dist && rimraf src && rimraf *.tsbuildinfo -g && rimraf \"./**/*.!(cmd|md|json|build.json|lts.json|tasks.json|cjs)\" -g",
        "precompile": "npm run build:dependencies",
        "build:dependencies": "npm run build -w @babylonjs/core-for-lottie-player",
        "compile": "tsc -b tsconfig.build.json",
        "postcompile": "build-tools -c add-js-to-es6",
        "prepack": "npm pack -w @babylonjs/core-for-lottie-player && node ./build/addDependency.cjs"
    },
    "devDependencies": {
        "@dev/build-tools": "^1.0.0",
        "@dev/core": "^1.0.0"
    }
}
