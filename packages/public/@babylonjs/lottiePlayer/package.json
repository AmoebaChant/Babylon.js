{
    "name": "@babylonjs/lottie-player",
    "version": "8.29.1",
    "main": "index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "esnext": "index.js",
    "description": "Babylon.js Lottie Player",
    "keywords": [
        "animation",
        "2D",
        "javascript",
        "html5",
        "webgl",
        "webgl2",
        "webgpu",
        "babylon",
        "lottie"
    ],
    "license": "MIT",
    "readme": "README.md",
    "type": "module",
    "sideEffects": true,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BabylonJS/Babylon.js.git"
    },
    "files": [
        "**/*.js",
        "**/*.d.ts",
        "**/*.map",
        "readme.md",
        "license.md"
    ],
    "scripts": {
        "build": "npm run clean && npm run consume:custom:core && npm run compile",
        "clean": "rimraf *.tsbuildinfo -g && rimraf \"./**/*.!(cmd|md|json|build.json|lts.json|tasks.json|cjs)\" -g",
        "consume:custom:core": "rimraf vendor && mkdir vendor && cd vendor && npm pack -w @babylonjs/core-for-lottie-player",
        "compile": "tsc -b tsconfig.build.json",
        "postcompile": "build-tools -c add-js-to-es6",
        "TODO": "TODO: before publishing to NPM, add a dependency from @babylonjs/core-for-lottie-player to file:vendor/babylonjs-core-for-lottie-player-1.0.0.tgz"
    },
    "devDependencies": {
        "@dev/build-tools": "^1.0.0",
        "@dev/core": "^1.0.0"
    },
    "peerDependencies": {
        "@babylonjs/core": "^7.47.3 || ^8.0.1"
    }
}
