{
    "name": "@babylonjs/smart-filters-blocks",
    "version": "8.10.1",
    "description": "Babylon.js Smart Filter Block Library",
    "keywords": [
        "video",
        "composition",
        "3D",
        "2D",
        "javascript",
        "html5",
        "webgl",
        "webgl2",
        "webgpu",
        "babylon"
    ],
    "license": "MIT",
    "readme": "README.md",
    "main": "dist/index",
    "module": "dist/index",
    "esnext": "dist/index",
    "types": "dist/index",
    "type": "module",
    "sideEffects": false,
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BabylonJS/Babylon.js.git"
    },
    "files": [
        "dist",
        "src",
        "license.md",
        "readme.md"
    ],
    "scripts": {
        "build": "npm run clean && npm run copySrc && npm run compile",
        "clean": "rimraf dist && rimraf src && rimraf *.tsbuildinfo -g && rimraf \"./**/*.!(cmd|md|json|build.json|lts.json|tasks.json|cjs)\" -g",
        "copySrc": "node -e \"require('fs').cpSync('../../../dev/smartFilterBlocks/src', './src', { recursive: true })\"",
        "precompile": "node ../../../dev/smartFilters/dist/utils/buildTools/buildShaders.js ./src/blocks @babylonjs/smart-filters",
        "compile": "tsc -b tsconfig.build.json"
    },
    "devDependencies": {
        "@babylonjs/core": "npm:@babylonjs/core@8.0.1",
        "@babylonjs/smart-filters": "^8.10.1",
        "@dev/build-tools": "^1.0.0",
        "@dev/smart-filters": "^1.0.1"
    },
    "peerDependencies": {
        "@babylonjs/core": "^8.0.1"
    },
    "dependencies": {
        "@babylonjs/smart-filters": "^8.10.1"
    }
}
