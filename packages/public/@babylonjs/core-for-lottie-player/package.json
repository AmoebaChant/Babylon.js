{
    "name": "@babylonjs/core-for-lottie-player",
    "version": "8.29.1",
    "main": "index.js",
    "module": "index.js",
    "types": "index.d.ts",
    "private": true,
    "files": [
        "**/*.js",
        "**/*.d.ts",
        "**/*.map",
        "**/*.wasm",
        "readme.md",
        "license.md",
        "NOTICE.md"
    ],
    "scripts": {
        "build": "npm run clean && npm run compile",
        "build:trimmed": "npm run build:trimmed -w @dev/core -- --trimConfig ./potato.json",
        "clean": "rimraf dist && rimraf *.tsbuildinfo -g && rimraf \"./**/*.!(cmd|md|json|build.json|lts.json|tasks.json|cjs)\" -g",
        "compile": "tsc -b tsconfig.build.json"
    },
    "devDependencies": {
        "@dev/build-tools": "^1.0.0",
        "@lts/core": "^1.0.0"
    },
    "type": "module",
    "sideEffects": [
        "**/*",
        "!src/Maths/ThinMaths/**"
    ]
}
