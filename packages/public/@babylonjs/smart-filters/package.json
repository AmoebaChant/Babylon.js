{
    "name": "@babylonjs/smart-filters",
    "version": "8.10.1",
    "description": "Babylon.js Smart Filter core",
    "keywords": [
        "video",
        "composition",
        "3D",
        "2D",
        "javascript",
        "html5",
        "webgl",
        "webgl2",
        "webgpu",
        "babylon"
    ],
    "license": "MIT",
    "readme": "README.md",
    "main": "dist/index",
    "module": "dist/index",
    "esnext": "dist/index",
    "types": "dist/index",
    "type": "module",
    "sideEffects": [
        "./dist/utils/buildTools/**"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/BabylonJS/SmartFilters.git"
    },
    "files": [
        "dist",
        "src",
        "license.md",
        "readme.md"
    ],
    "scripts": {
        "build": "npm run clean && npm run compile",
        "clean": "rimraf dist && rimraf *.tsbuildinfo -g && rimraf \"./**/*.!(cmd|md|json|build.json|lts.json|tasks.json|cjs)\" -g",
        "compile": "tsc -b tsconfig.build.json && node dist/utils/buildTools/recordVersionNumber.js"
    },
    "devDependencies": {
        "@dev/build-tools": "^1.0.0"
    },
    "peerDependencies": {
        "@babylonjs/core": "^8.0.1"
    }
}
